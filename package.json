{
  "name": "photobooth",
  "version": "0.0.0",
  "description": "a pics/gifs capture webapp",
  "main": "index.js",
  "dependencies": {
    "angular": "^1.4.1",
    "babel": "^5.6.14"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.13",
    "browserify": "^10.2.4",
    "opener": "^1.4.1",
    "partialify": "^3.1.3",
    "uglify-js": "^2.4.23",
    "watchify": "^3.2.3"
  },
  "scripts": {
    "build": "browserify ./app -o bundle.js",
    "build-min": "browserify ./app | uglifyjs -mc > bundle.js",
    "watch": "watchify ./app -v -o bundle.js",
    "syncserve": "browser-sync start --server --files \"bundle.js, style.css\"",
    "dev": "npm run build && npm run syncserve & npm run watch",
    "start": "npm run build-min && npm run syncserve"
  },
  "author": "Mathieu Dubois",
  "license": "MIT",
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "babelify",
      "partialify"
    ]
  }
}
