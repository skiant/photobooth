{
  "name": "photobooth",
  "version": "0.0.0",
  "description": "a pics/gifs capture webapp",
  "main": "index.js",
  "dependencies": {
    "angular": "^1.4.1"
  },
  "devDependencies": {
    "opener": "^1.4.1",
    "partialify": "^3.1.3",
    "uglify-js": "^2.4.23",
    "browserify": "^10.2.4",
    "babelify": "^6.1.2"
  },
  "scripts": {
    "build-js": "browserify ./app | uglifyjs -mc > bundle.js",
    "dev-js": "browserify ./app -o bundle.js",
    "start": "npm run build-js && opener index.html"
  },
  "author": "Mathieu Dubois",
  "license": "MIT",
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "babelify",
      "partialify"
    ]
  }
}
